# Interactive Brokers Configuration
IB_USER=your_ib_username
IB_PASSWORD=your_ib_password

# Trading Configuration
TRADING_MODE=paper
ACCEPT_NON_BROKERAGE_WARNING=yes
READ_ONLY_API=no
TIME_ZONE=America/New_York

# VNC Configuration
VNC_SCREEN_DIMENSION=1600x1200x16

# IBC Configuration
TWOFA_TIMEOUT_ACTION=exit
AUTO_RESTART_TIME=
AUTO_LOGOFF_TIME=
COLD_RESTART_TIME=
BYPASS_WARNING=yes
SAVE_TWS_SETTINGS=Every 30 mins
RELOGIN_AFTER_TWOFA_TIMEOUT=no
TWOFA_EXIT_INTERVAL=60

# Port Forwarding Configuration
# Set to "no" if using host networking and only need localhost access
ENABLE_SOCAT=yes

# SSH Tunnel Configuration (Optional)
# Enable SSH tunnel: "yes" (SSH only), "both" (SSH + local), or leave empty to disable
SSH_TUNNEL=
# SSH connection string (user@server)
SSH_USER_TUNNEL=
# Remote port for SSH tunnel (defaults to API port)
SSH_REMOTE_PORT=
# Remote port for VNC tunnel (Gateway only)
SSH_VNC_PORT=
# Additional SSH options
SSH_OPTIONS=
# SSH keep-alive settings
SSH_ALIVE_INTERVAL=20
SSH_ALIVE_COUNT=3
# SSH key passphrase (use SSH_PASSPHRASE_FILE for production)
SSH_PASSPHRASE=
SSH_PASSPHRASE_FILE=
# Restart delay for failed tunnels
SSH_RESTART=5

# Supervisord Web Interface (Optional)
SUPERVISORD_UI_PORT=
SUPERVISORD_UI_USER=
SUPERVISORD_UI_PASS=
